.btn
  padding: .5rem 1rem !important
  background: transparent
  font-size: 1rem
  height: 35px
  border-radius: 35px
  text-decoration: none
  transition: all .3s ease-out
  cursor: pointer
  &:hover
    text-decoration: none !important

.btn-charcoal
  color: $white
  background: $charcoal
  &:hover
    background: $green